#include <stdio.h>

void constructPermutation(int N) {
    
    int P[N];
    

    for (int i = 0; i < N; i++) {
        P[i] = N - i;
    }

  
    for (int i = 0; i < N; i++) {
        printf("%d ", P[i]);
    }
    printf("\n");
}

int main() {
    int N;
    scanf("%d", &N);
    
    constructPermutation(N);
    
    return 0;
}
