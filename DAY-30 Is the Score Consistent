#include <stdio.h>

int main() {
    int A, B, C, D;

    printf("Enter the current score A:B: ");
    scanf("%d:%d", &A, &B);

    printf("Enter the desired score C:D: ");
    scanf("%d:%d", &C, &D);

    if (C >= A && D >= B) {
        printf("Yes, it is possible to reach the score %d:%d.\n", C, D);
    } else {
        printf("No, it is not possible to reach the score %d:%d.\n", C, D);
    }

    return 0;
}
