#include <stdio.h>

int main() {
    int X, Y;

    printf("Enter the rental cost per month (X): ");
    scanf("%d", &X);
    
    printf("Enter the purchase cost (Y): ");
    scanf("%d", &Y);

    int max_months = (Y - 1) / X; 
    
    printf("The maximum number of months for which Chef can rent the cooler is: %d\n", max_months);
    
    return 0;
}
