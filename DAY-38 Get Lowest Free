#include <stdio.h>

int main() {
    int A, B, C;
    
   
    scanf("%d %d %d", &A, &B, &C);
   
    int total = A + B + C;
    
    int min_price = A;
    if (B < min_price) min_price = B;
    if (C < min_price) min_price = C;
    
    int amount_to_pay = total - min_price;
    
    printf("%d\n", amount_to_pay);
    
    return 0;
}
