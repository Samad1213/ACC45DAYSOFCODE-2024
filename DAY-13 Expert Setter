#include <stdio.h>

int main() {
    int X, Y;

    printf("Enter the total number of problems submitted (X): ");
    scanf("%d", &X);
    printf("Enter the number of problems approved (Y): ");
    scanf("%d", &Y);

    if (X > 0) {
        if (Y >= (X / 2)) {
            printf("Munchy is an expert.\n");
        } else {
            printf("Munchy is not an expert.\n");
        }
    } else {
        printf("Total submitted problems should be greater than 0.\n");
    }

    return 0;
}
