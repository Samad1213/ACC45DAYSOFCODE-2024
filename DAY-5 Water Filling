#include <stdio.h>

int main() {
    int B1, B2, B3;

    printf("Enter the status of three bottles (0 for empty, 1 for full): ");
    scanf("%d %d %d", &B1, &B2, &B3);

    int empty_count = (B1 == 0) + (B2 == 0) + (B3 == 0);

    if (empty_count >= 2) {
        printf("Water filling time\n");
    } else {
        printf("Not now\n");
    }

    return 0;
}
